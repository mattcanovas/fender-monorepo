<Page x:Class="Fender.App.Views.Agenda.AgendaConsultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Fender.App.Views.Agenda"
      mc:Ignorable="d"
      Title="AgendaConsultPage"
      d:DesignWidth="1280"
      d:DesignHeight="720">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        
        <Label Grid.Row="0" Grid.Column="0" Content="Consultar agendar" Style="{StaticResource PrimaryLabel}" />
        <ListView Grid.Row="1" Grid.Column="0" />
    </Grid>
</Page>
